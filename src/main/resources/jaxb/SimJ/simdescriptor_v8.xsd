            <xs:attribute name="hideclipmedia" type="xs:int" default="0" />

            <!-- the id of this sim's underlying IMO -->
            <xs:attribute name="imonum" type="xs:long" default="0" />

            <!-- Indicates that Scored Biodata scores should be included in the overall score. -->
            <xs:attribute name="includebiodataoverall" type="xs:int" default="0" />

            <!-- Indicates that General (not task, interest, experience, biodata, aggregate)
            Sim Competency scores should be included in the overall score. -->
            <xs:attribute name="includecompetenciesoverall" type="xs:int" default="0" />

            <!-- Indicates that the system should include Sim Competency Scores that have any data at all, like interview questions,
                 Even if they don't have any items associated with them.
            -->
            <xs:attribute name="inclcompswnoitmsinscrdata" type="xs:int" default="0" />

            <!-- Indicates that Scored Interest Competency scores should be included in the overall score. -->
            <xs:attribute name="includeinterestoverall" type="xs:int" default="0" />

            <!-- Indicates that Scored Experience scores should be included in the overall score. -->
            <xs:attribute name="includeexperienceoverall" type="xs:int" default="0" />

            <!-- Indicates the way interview questions should be ordered in a report. Values:
              DEFAULT(0,"Default - Order by Competency Rank"),    // When an ONET Soc is selected
              SCORE_DESC(1,"Order by Competency Score Lowest to Highest"),
              SCORE_ASC(2,"Order by Competency Score Highest to Lowest"),
              IMPORTANCT(3,"Order By Competency Importance Highest to Lowest")
            -->
            <xs:attribute name="interviewquesordertypeid" type="xs:int" default="0" />

            <!-- CT5 - Items per clip, for randomization -->
            <xs:attribute name="itemgroupsize" type="xs:int" default="0" />


            <!-- The languagge used by this sim. -->
            <xs:attribute name="lang" type="xs:string" default="en_US"/>

            <!-- The date of the last sim competency statistics update -->
            <xs:attribute name="lastsimcompetencystatupdate" type="xs:long" default="0"/>

            <!-- CT5 - for Game Types - Indicates the points required to advance to the next level  -->
            <xs:attribute name="levelmax" type="xs:int" default="0" />

            <!-- major version id of this sim. This is used to demarcate between major version changes. Used in HR Avatar scoring system as a barrier between percentiles and score stats calculations. -->
            <xs:attribute name="majorversionid" type="xs:int" default="1" />

            <!-- This is the minimum sim versionid matching the current majorversionid of this sim. -->
            <xs:attribute name="minsimveridformajorversion" type="xs:int" default="0" />

            <!-- mean - used when overall scores are calculated using a normalized scale. This value is used to convert the overall raw score to a z score. -->
            <xs:attribute name="mean" type="xs:float" default="0" />

            <!-- Indicates how multiple competencies per item are represented in the IMO and how they should be scored.
              ONE_PER 0 - only one competency per item type. Points stored in the 'itemscore' field.
              PKSA 1 - Competency, Knowledge, Skills, Abilities - four different scores stored in the itemscore, score1, score2, and score3 fields.
            -->
            <xs:attribute name="multicompetenciestypeid" type="xs:int" default="1" />

            <!-- The name of this sim on the authoring system. -->
            <xs:attribute name="name" type="xs:string" />

            <!-- name in English. Useful when dealing with multiple languages -->
            <xs:attribute name="nameenglish" type="xs:string" />

            <!-- The name of this sim to show the user. -->
            <xs:attribute name="nameuser" type="xs:string" />

            <!--
                 indicates that the Sim perform native reporting back to
                 the clicflic server in addition to any other reporting done.
            -->
            <xs:attribute name="nativerpt" type="xs:int" default="0" />

            <!-- CT5 - Default text for Next button -->
            <xs:attribute name="nextbuttontext"  type="xs:string" />



            <!-- the onet version used to create competencies and tasks for this sim, if any. -->
            <xs:attribute name="onetversion" type="xs:string" />

            <!--
                 Determines how RAW overall score is calculated from all subscores.
      
                 0=Use user-specified weights for each task and competency.
                 1=Use Onet Importance values as weights.
                 2=Use User Rank values as weights.
            -->
            <xs:attribute name="overallscorecalctype" type="xs:int" default="0" />

            <!--
                 Determines how the SCALED or final overall score is calculated from the raw score.
      
                 0=Use raw score with no changes.
                 1=Use lookup table if present
                 2=Convert Raw to Z (using mean and standard deviation provided in mean and standarddeviation) and then scale using Normal Equivalent Curve values (mean=50, Std Dev = 21.06)
                 3=Convert Raw to Z and then scale using provided mean and std deviation in rawtoscaledfloatparam1 and rawtoscaledfloatparam2
      
            -->
            <xs:attribute name="overallscaledscorecalctype" type="xs:int" default="0" />


            <!-- CT5 - Progress Bar format 0=% complete, 1=X of Y -->
            <xs:attribute name="percentcompleteformat" type="xs:int" default="0" />


            <!--
                 If using overallscaledscorecalctype=3, floatparam1 below is the target mean and floatparam2 is the target standard deviation to convert to.
      
                 rawtoscaledfloatparam 1 and 2 are mean/sd for OVERALL SCALED SCORE
                 rawtoscaledfloatparam 3 and 4 are mean/sd for OVERALL RAW SCORE
      
            -->
            <xs:attribute name="rawtoscaledfloatparam1" type="xs:float" default="0" />
            <xs:attribute name="rawtoscaledfloatparam2" type="xs:float" default="0" />
            <xs:attribute name="rawtoscaledfloatparam3" type="xs:float" default="0" />
            <xs:attribute name="rawtoscaledfloatparam4" type="xs:float" default="0" />


            <!-- The default report id for this Sim
            CT5 - Custom Report Id -->
            <xs:attribute name="report" type="xs:long" default="0" />

            <!-- Required Recording Devices Code
                 0=none
                 1=audio only
                 2=camera only
                 3=microphone and camera
            -->
            <xs:attribute name="reqrecdevs" type="xs:int" default="0" />



            <!-- CT5 - Instructions for restart. 0=restart where left off, 1=restart on last answered item, 10=restart at beginning. -->
            <xs:attribute name="restarttype" type="xs:int" default="0" />

            <!--  The score color scheme type. Values are:
                THREE COLORS (0),
                FIVE COLORS(1)
            -->
            <xs:attribute name="scorecolorscheme" type="xs:int" default="0" />


            <!--  The score format type. Values are:
                NUMERIC_1_TO_5(0,"1-5"),
                NUMERIC_0_TO_100(1,"0-100")
            -->
            <xs:attribute name="scoreformat" type="xs:int" default="0" />

            <!-- CT5 - Shuffle groups of shuffle-able items -->
            <xs:attribute name="shuffleitemgroups" type="xs:int" default="0" />



            <!-- id of this sim. -->
            <xs:attribute name="simid" type="xs:long" default="0" />

            <!-- indicates the way sim competencies should be sorted in reports (within groups)
                 0 = default = weights (if present) then name
                 1 = user rank, then name
                 3 = dispord, then name
            -->
            <xs:attribute name="simcompetencyreportingsorttype" type="xs:int" default="0" />

            <!-- id of the sim template used to build this sim. -->
            <xs:attribute name="simtemplate" type="xs:long" default="0" />

            <!-- version id of this sim. -->
            <xs:attribute name="simverid" type="xs:int" default="0" />

            <!-- The date the current version of this Sim was initially built. -->
            <xs:attribute name="simverdate" type="xs:long" default="0"/>

            <!-- The number of this build within this version. Sequential starting from 0. Incremented every time a sim is built within this sim version.  -->
            <xs:attribute name="simverbldnum" type="xs:int" default="0"/>

            <!-- Onet standard occupational code assigned to this Sim, if any. -->
            <xs:attribute name="soc" type="xs:string" />

            <!-- stddeviation - used when overall scores are calculated using a normalized scale. This value is used to convert the overall raw score to a z score. -->
            <xs:attribute name="stddeviation" type="xs:float" default="0" />

            <!-- URL to the SWF file compiled when this sim was built. -->
            <xs:attribute name="swfurl" type="xs:string" />

            <!-- Overall Time Limit in seconds. Note that this value can be changed by interactions and clips within a sim,
            so it's really just the initial time limit.
            CT5 - time limit in seconds -->
            <xs:attribute name="timeoutoverall" type="xs:float" default="0" />

            <!-- Indicates the trype of training needed to perform in this job. Values are:
                NOT_SET(0,"None"),
                TT_0_6M(1,"Less 6 Months"),
                TT_6M_1(2,"6 Months - 1 Year"),
                TT_1_2(3,"1 - 2 Years"),
                TT_2_4(4,"2 - 4 Years"),
                TT_4_10(5,"4 - 10 Years"),
                TT_10(6,"Over 10 Years");
            -->
            <xs:attribute name="trn" type="xs:int" default="0" />

            <!-- Type of Sim. Values are:
            ONET(1,"O*Net-Based"),
            NON_ONET(2,"Not O*Net-Based")
            -->
            <xs:attribute name="type" type="xs:int" default="0" />

            <!-- Indicates that the unique ids for interactions have been modified for trace-back to their source interactions within simlets.
                 This can be useful for performing item analysis across different sims.  Note, however, that each interaction contained in this
                 document can be traced back to its original simlet without the uniqueid.
      
                 0=no change
                 1=replace all simlet interaction uniqueids with simletid-simletVersionId-simlet.interaction.sequenceId
                 2=append a prefix to each simlet interaction unique id using prefix=
            -->
            <xs:attribute name="uniqueidchangetypeid" type="xs:int" default="0" />

            <!-- Indicates the method for calculating SimCompetency scores across multiple simlets.
                 0=Calculate competency scores individually and combine evenly.
                 1=Calculate competency scores individually and combine using Simlet Competency Coverage Values
                 2=Calculate competency scores across all simlets at the same time.
            -->
            <xs:attribute name="usesmltcompcvrg" type="xs:int" default="0" />

            <!--
                  A value of 1 indicates this sim uses standard HRA Scoring.
            -->
            <xs:attribute name="usesstdhrascoring" type="xs:int" default="0" />

            <!-- Indicates that scoring algorithm should use the total items in the sim or simlet (depending on what is being scored) to calculate
                 the total value (either total points available, or total correct items possible) when scoring. The default value (0) means only use
                 items that have been encountered (responded to) by the test taker.
            -->
            <xs:attribute name="usetotalitemsforcompcalcs" type="xs:int" default="0" />


            <!-- indicates that the userrank values of each sim competency and sim task should be used as the weight instead of the weight field.
            12/27/2012 - removed and incorporated into overallscorecalctype -->
            <!-- <xs:attribute name="userankvaluesasweights" type="xs:int" default="0" /> -->

            <!-- width of IMO in pixels -->
            <!-- width of Ct5Direct Test in pixels -->
            <xs:attribute name="width" type="xs:int" default="0" />

            <!-- min score for white range. -->
            <xs:attribute name="whitemin" type="xs:float" default="0" />

            <!-- min score for green range. -->
            <xs:attribute name="greenmin" type="xs:float" default="0" />

            <!-- min score for yellow-green range. -->
            <xs:attribute name="yellowgreenmin" type="xs:float" default="0" />

            <!-- min score for yellow range (below this is red range) -->
            <xs:attribute name="yellowmin" type="xs:float" default="0" />

            <!-- min score for red-yellow range. -->
            <xs:attribute name="redyellowmin" type="xs:float" default="0" />

            <!-- min score for red-yellow range. -->
            <xs:attribute name="redmin" type="xs:float" default="0" />

        </xs:complexType>
    </xs:element>

    <xs:complexType name="interviewQuestionObj">
        <xs:sequence>

            <!--  -->
            <xs:element name="question" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
            <xs:element name="anchor" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:simpleContent msdata:ColumnName="anchor_Text" msdata:Ordinal="1">
                        <xs:extension base="xs:string">

                            <!--  -->
                            <xs:attribute name="type" type="xs:int" />  <!-- hi, med, low -->
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>

        <!--
          Defines the breadth of score range for which this quetion is applicable
  
          0 = Default - for use in all cases.
          5 = Question is preferred for Low Red scores
          10 = Question is preferred for all Low scores
          15 = Question is preferred for Medium scores
          20 = Question is preferred for all High scores
          25 = Question is preferred for High red scores
        -->
        <xs:attribute name="breadth" type="xs:int" default="0" />

        <!--  -->
        <xs:attribute name="onetversion" type="xs:string" />

        <!--  -->
        <xs:attribute name="soc" type="xs:string" />

        <xs:attribute name="type" type="xs:int" default="0" />
    </xs:complexType>


</xs:schema>